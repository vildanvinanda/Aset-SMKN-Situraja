<?xml version="1.0" encoding="utf-8"?>

<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="110dp"
    app:cardCornerRadius="10dp"
    android:elevation="10dp"
    android:translationZ="2dp"
    app:cardUseCompatPadding="true"
    android:layout_marginHorizontal="15dp"
    android:id="@+id/itemlistuser"

    >

    <RelativeLayout
    android:id="@+id/bgcard2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:padding="15dp">

    <androidx.cardview.widget.CardView
        android:id="@+id/tamplateimg_user"
        android:layout_width="63dp"
        android:layout_height="63dp"
        app:cardCornerRadius="10dp"

        >

        <ImageView
            android:id="@+id/img_user"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:background="@drawable/ic_prof_user" />
    </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/tamplateimg_user"
            android:layout_marginLeft="10dp"
            >
            <RelativeLayout
                android:id="@+id/tamplatetxt4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:id="@+id/txtnjml"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="Nama"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_medium"
                    />
                <TextView
                    android:id="@+id/tikdu4"
                    android:layout_toEndOf="@+id/txtnjml"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_medium"
                    />
                <TextView
                    android:id="@+id/addnama"
                    android:layout_marginLeft="10dp"
                    android:layout_toEndOf="@+id/tikdu4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="***********"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_regular"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/tamplatetxt4"
                android:id="@+id/tamplatetxt5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >
                <TextView
                    android:id="@+id/txtjtp"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="NINS"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_medium"
                    />
                <TextView
                    android:id="@+id/tikdu5"
                    android:layout_toEndOf="@+id/txtjtp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_medium"
                    />
                <TextView
                    android:id="@+id/addnis"
                    android:layout_marginLeft="10dp"
                    android:layout_toEndOf="@+id/tikdu5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="***********"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_regular"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:layout_below="@+id/tamplatetxt5"
                android:id="@+id/tamplatetxt6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >
                <TextView
                    android:id="@+id/txtket"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="Kelas"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_medium"
                    />
                <TextView
                    android:id="@+id/tikdu6"
                    android:layout_toEndOf="@+id/txtket"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_medium"
                    />
                <TextView
                    android:id="@+id/addkls"
                    android:layout_marginLeft="10dp"
                    android:layout_toEndOf="@+id/tikdu6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="***********"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/roboto_regular"
                    />
            </RelativeLayout>
        </RelativeLayout>




</RelativeLayout>

</androidx.cardview.widget.CardView>